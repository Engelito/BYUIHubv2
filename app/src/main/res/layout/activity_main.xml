<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.byuihub.byuistudenthub.MainActivity">

    <ImageButton
        android:id="@+id/links"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:onClick="navLinks"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_bookmark"
        app:layout_constraintVertical_bias="0.698" />

    <ImageButton
        android:id="@+id/todo"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginRight="-69dp"
        android:onClick="navTodo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/links"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_check_box"
        app:layout_constraintVertical_bias="0.447" />

    <ImageButton
        android:id="@+id/map"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="-19dp"
        android:onClick="navMap"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/links"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_place"
        app:layout_constraintVertical_bias="0.447" />
</android.support.constraint.ConstraintLayout>
